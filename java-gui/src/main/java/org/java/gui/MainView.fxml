        <tabs>
          <Tab fx:id="selectProjectTab" text="프로젝트 선택">
               <content>
                    <fx:include source="selectproject/selectProjectContainer.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
               
                  <!--<AnchorPane fx:id="selectProjectContainer" prefHeight="200.0" prefWidth="200.0" />-->
               </content>
            </Tab>
          <Tab fx:id="selectRulesTab" text="룰 선택">
               <content>
               		<fx:include source="selectrules/selectRulesContainer.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
               
                  <!-- <AnchorPane fx:id="selectRulesContainer" prefHeight="200.0" prefWidth="200.0" /> -->
