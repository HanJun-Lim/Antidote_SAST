            </Tab>
            <Tab fx:id="analysisStatisticsTab" text="분석 통계">
              <content>
              		<fx:include source="analysisstatistics/analysisStatisticsContainer.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
              
